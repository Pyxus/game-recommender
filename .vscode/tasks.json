{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "run",
			"options": {
				"cwd": "${workspaceFolder}/server"
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo run"
		},
		{
			"type": "cargo",
			"command": "build",
			"options": {
				"cwd": "${workspaceFolder}/server"
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		}
	]
}